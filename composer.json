{
    "name": "iracode/assets",
    "description": "Laravel assets management",
    "license": "MIT",
    "keywords": [
        "iracode",
        "iracode cms",
        "iracode platform",
        "iracode assets"
    ],
    "homepage": "https://iracode.com",
    "authors": [
        {
            "name": "Sang Nguyen",
            "email": "sangnguyenplus@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0|^8.1",
        "laravel/framework": "^5.6|^6|^7|^8|^9"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Iracode\\Assets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iracode\\Assets\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Iracode\\Assets\\Providers\\AssetsServiceProvider"
            ],
            "aliases": {
                "Assets": "Iracode\\Assets\\Facades\\AssetsFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
